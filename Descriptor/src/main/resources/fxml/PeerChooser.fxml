<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="-1.0" prefWidth="-1.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="example.subspace.chat.javafx.PeerChooserController">
  <bottom>
    <HBox alignment="BOTTOM_RIGHT" prefHeight="-1.0" prefWidth="-1.0" spacing="20.0" BorderPane.alignment="CENTER">
      <children>
        <Button fx:id="okButton" minWidth="50.0" mnemonicParsing="false" text="OK" />
        <Button fx:id="cancelButton" minWidth="50.0" mnemonicParsing="false" text="Cancel" />
      </children>
      <padding>
        <Insets />
      </padding>
      <BorderPane.margin>
        <Insets top="20.0" fx:id="x1" />
      </BorderPane.margin>
    </HBox>
  </bottom>
  <center>
    <ChoiceBox fx:id="choiceBox" minWidth="200.0" BorderPane.margin="$x1" />
  </center>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <stylesheets>
    <URL value="@styles/Styles.css" />
  </stylesheets>
  <top>
    <Label styleClass="headline" text="Choose a Person:" BorderPane.alignment="BOTTOM_LEFT" />
  </top>
</BorderPane>
